{
  "name": "doc-mgt-system",
  "version": "1.0.0",
  "description": "document management system with access rights",
  "main": "server.js",
  "scripts": {
    "prestart": "babel-node ./tools/startMessage.js",
    "start": "npm-run-all --parallel open:src",
    "open:src": "babel-watch ./tools/srcServer.js",
    "lint": "eslint .",
    "lint:watch": "npm run lint -- --watch",
    "test": "istanbul cover _mocha -- -R spec test/**/*.spec.js",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:seed": "node_modules/.bin/sequelize db:seed:all",
    "test:db:migrate": "NODE_ENV=test node_modules/.bin/sequelize db:migrate",
    "test:db:migrate:undo:all": "NODE_ENV=test node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-oagunbiade/doc-mgt-system.git"
  },
  "keywords": [
    "document",
    "management",
    "system",
    "api",
    "javascript"
  ],
  "author": "ore agunbiade",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-oagunbiade/doc-mgt-system/issues"
  },
  "homepage": "https://github.com/andela-oagunbiade/doc-mgt-system#readme",
  "dependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-polyfill": "6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.8.0",
    "babel-watch": "^2.0.6",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "bootstrap": "3.3.6",
    "chai": "^3.5.0",
    "cheerio": "0.22.0",
    "colors": "1.1.2",
    "compression": "1.6.1",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "dotenv": "^4.0.0",
    "enzyme": "2.2.0",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.19.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "faker": "^3.1.0",
    "file-loader": "0.8.5",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.2.1",
    "jquery": "2.2.3",
